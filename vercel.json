{
  "buildCommand": "",
  "outputDirectory": ".",
  "rewrites": [
    { "source": "/admin", "destination": "/admin.html" },
    { "source": "/post", "destination": "/post.html" },
    {
      "source": "/",
      "has": [{ "type": "host", "value": "phillesports.com" }],
      "destination": "/esports/"
    },
    {
      "source": "/",
      "has": [{ "type": "host", "value": "www.phillesports.com" }],
      "destination": "/esports/"
    },
    {
      "source": "/:path*",
      "has": [{ "type": "host", "value": "phillesports.com" }],
      "destination": "/esports/:path*"
    },
    {
      "source": "/:path*",
      "has": [{ "type": "host", "value": "www.phillesports.com" }],
      "destination": "/esports/:path*"
    }
  ],
  "crons": [
    {
      "path": "/api/bets/score",
      "schedule": "*/15 * * * *"
    }
  ]
}
